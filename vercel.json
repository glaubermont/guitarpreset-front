{
  "version": 2,
  "builds": [
    {
      "src": "dist/guitarpreset/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      },
      "dest": "/dist/guitarpreset/index.html"
    }
  ]
}
