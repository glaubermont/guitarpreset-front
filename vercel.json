{
  "version": 2,
  "builds": [
    {
      "src": "dist/guitarpreset/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {

      "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|woff2?|json|ico))$",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      },
      "dest": "/$1"
    },
    {
   
      "src": "/api/(.*)$",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      },
      "dest": "https://wicked-terza-guitarpreset-4522c1b5.koyeb.app/api/backingtracks"
    },
    {
    
      "src": "/(.*)",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      },
      "dest": "/index.html"
    }
  ]
}
